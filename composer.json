{
    "name": "friendsofsymfony/elastica-bundle",
    "type": "symfony-bundle",
    "description": "Elasticsearch PHP integration for your Symfony2 project using Elastica",
    "keywords": ["doctrine2", "elastica", "elasticsearch", "mongodb", "propel", "search"],
    "homepage": "https://github.com/FriendsOfSymfony/FOSElasticaBundle",
    "license": "MIT",
    "authors": [
        { "name": "FriendsOfSymfony Community", "homepage": "https://github.com/FriendsOfSymfony/FOSElasticaBundle/contributors" },
        { "name": "Tim Nagel", "email": "tim@nagel.com.au" },
        { "name": "Richard Miller", "email": "richard.miller@limethinking.co.uk" },
        { "name": "Jeremy Mikola", "email": "jmikola@gmail.com" }
    ],
    "require": {
        "php": ">=5.6.0",
        "symfony/framework-bundle": "^2.7 || ^3.1",
        "symfony/console": "^2.7 || ^3.1",
        "symfony/form": "^2.7 || ^3.1",
        "symfony/property-access": "^2.7 || ^3.1",
        "symfony/asset": "^2.7 || ^3.1",
        "symfony/templating": "^2.7 || ^3.1",
        "symfony/translation": "^2.7 || ^3.1",
        "psr/log": "^1.0",
        "ruflin/elastica": "^5.1"
    },
    "require-dev":{
        "doctrine/orm": "^2.4",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/phpcr-bundle": "^1.2",
        "jackalope/jackalope-doctrine-dbal": "^1.1",
        "jms/serializer-bundle": "^1.1",
        "phpunit/phpunit": "^4.8 || ^5.0",
        "propel/propel1": "1.6.*",
        "pagerfanta/pagerfanta": "^1.0",
        "knplabs/knp-components": "^1.2",
        "knplabs/knp-paginator-bundle": "^2.4",
        "symfony/browser-kit" : "^2.7 || ^3.1",
        "symfony/expression-language" : "^2.7 || ^3.1",
        "symfony/twig-bundle": "^2.7 || ^3.1",
        "symfony/serializer": "^2.7 || ^3.1",
        "symfony/yaml": "^2.7 || ^3.1",
        "symfony/dependency-injection": "^2.7 || ^3.1",
        "friendsofphp/php-cs-fixer": "^1.11"
    },
    "autoload": {
        "psr-4": { "FOS\\ElasticaBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.0.x-dev"
        }
    }
}
